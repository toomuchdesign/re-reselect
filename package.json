{
  "name": "re-reselect",
  "version": "2.1.0",
  "description": "Enhance Reselect selectors with deeper memoization and cache management",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": "dist/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "src"
  ],
  "scripts": {
    "test": "npm run test:typescript && jest",
    "test:typescript": "typings-tester --dir typescript_test",
    "test:bundles": "jest --config ./jest/es.config.js && jest --config ./jest/lib.config.js && jest --config ./jest/dist.config.js",
    "clean": "rimraf dist es lib",
    "compile": "npm run clean && cross-env BABEL_ENV=rollup rollup -c",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate",
    "preversion": "npm run prepublish",
    "version": "git add package.json",
    "postversion": "git push && git push --tags",
    "prepublish": "npm test -- --coverage && npm run compile && npm run test:bundles",
    "precommit": "lint-staged",
    "format": "prettier --write \"**/*.{js,ts,json,md}\""
  },
  "keywords": [
    "react",
    "redux",
    "reselect",
    "memoize"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toomuchdesign/re-reselect.git"
  },
  "author": "Andrea Carraro <me@andreacarraro.it>",
  "license": "MIT",
  "peerDependencies": {
    "reselect": ">1.0.0"
  },
  "devDependencies": {
    "all-contributors-cli": "^4.11.1",
    "babel-core": "^6.23.1",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "coveralls": "^3.0.1",
    "cross-env": "^5.0.5",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.11.1",
    "reselect": ">1.0.0",
    "rimraf": "^2.6.1",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.3",
    "typescript": "^2.7.2",
    "typings-tester": "^0.2.2"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "__util__"
    ],
    "testEnvironment": "node"
  },
  "sideEffects": false
}
